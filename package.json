{
  "name": "nusantara-valid",
  "version": "0.1.0",
  "description": "Nusantara Valid is a Indonesian civil data validator.",
  "main": "dist/nusantara-valid.js",
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "validator",
    "javascript",
    "indonesia",
    "nik",
    "nip",
    "nisn",
    "npwp",
    "tnkb",
    "postcode",
    "email",
    "mobile-number",
    "telephone-number"
  ],
  "scripts": {
    "clean": "shx rm -rf dist src/js",
    "compile": "npm-run-all --sequential clean compile:ts compile:js",
    "compile:js": "rollup --config rollup.config.js --sourcemap",
    "compile:ts": "tsc --module ESNext",
    "test": "mocha --require ts-node/register **/*.test.ts"
  },
  "author": "magicjar",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.16.1",
    "shx": "^0.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
